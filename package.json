{
  "name": "illog",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "build:packages": "pnpm --filter './packages/*' build",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "keywords": [],
  "author": "youaresoyoung",
  "license": "ISC",
  "packageManager": "pnpm@10.12.3",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.sql": [
      "prettier --write --plugin=prettier-plugin-sql"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "esbuild": "^0.25.12",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "3.6.2",
    "prettier-plugin-sql": "^0.19.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.0.2"
  }
}
